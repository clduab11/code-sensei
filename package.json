{
  "name": "code-sensei",
  "version": "1.0.0",
  "description": "Intelligent GitHub bot for automated code review with AI-powered analysis and auto-fixes",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t code-sensei .",
    "docker:run": "docker run -p 3000:3000 code-sensei"
  },
  "keywords": [
    "github",
    "bot",
    "code-review",
    "ai",
    "probot",
    "automation",
    "static-analysis",
    "security"
  ],
  "author": "Code Sensei Team",
  "license": "MIT",
  "dependencies": {
    "probot": "^13.0.0",
    "@anthropic-ai/sdk": "^0.27.0",
    "@octokit/rest": "^20.0.0",
    "@octokit/webhooks": "^12.0.0",
    "express": "^4.18.0",
    "dotenv": "^16.3.0",
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "zod": "^3.22.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "eslint": "^8.0.0",
    "pylint-json2html": "^0.3.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "acorn": "^8.11.0",
    "acorn-walk": "^8.3.0",
    "tree-sitter": "^0.20.0",
    "tree-sitter-python": "^0.20.0",
    "tree-sitter-typescript": "^0.20.0",
    "tree-sitter-go": "^0.20.0",
    "tree-sitter-rust": "^0.20.0",
    "snyk": "^1.1290.0",
    "semver": "^7.5.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "bull": "^4.11.0",
    "@slack/web-api": "^6.10.0",
    "jira-client": "^8.2.0",
    "@linear/sdk": "^12.0.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.0",
    "prom-client": "^15.0.0",
    "node-cache": "^5.1.0",
    "diff": "^5.1.0",
    "fast-diff": "^1.3.0",
    "minimatch": "^9.0.0",
    "glob": "^10.3.0",
    "yaml": "^2.3.0",
    "jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/diff": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.0",
    "smee-client": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
