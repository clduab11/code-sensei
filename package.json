{
  "name": "code-sensei",
  "version": "1.0.0",
  "description": "AI-powered GitHub bot for automated code review & auto-fix commits",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "github-app",
    "code-review",
    "ai",
    "claude",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@octokit/rest": "^20.0.2",
    "probot": "^13.0.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "@babel/parser": "^7.23.5",
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "^14.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.42",
    "@typescript-eslint/parser": "^6.13.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "tsx": "^4.6.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
